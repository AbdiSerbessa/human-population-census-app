{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "widows",
          "-webkit-overflow-scrolling",
          "overflow-anchor",
          "orphans",
          "@page",
          "-ms-filter"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.4",
    "not safari <= 16.4"
  ]
}